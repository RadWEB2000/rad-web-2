{
  "kind": "collectionType",
  "collectionName": "people",
  "info": {
    "singularName": "person",
    "pluralName": "people",
    "displayName": "Person",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "title": {
      "type": "string",
      "required": true
    },
    "email": {
      "type": "email"
    },
    "featuredImage": {
      "type": "media",
      "multiple": false,
      "required": true,
      "allowedTypes": [
        "images"
      ]
    },
    "editor": {
      "type": "customField",
      "options": {
        "preset": "toolbar"
      },
      "customField": "plugin::ckeditor5.CKEditor"
    },
    "slug": {
      "type": "customField",
      "options": {
        "pattern": "title"
      },
      "customField": "plugin::slug.slug"
    },
    "seo": {
      "type": "component",
      "repeatable": false,
      "component": "shared.seo"
    },
    "phone": {
      "type": "string",
      "required": true
    },
    "website": {
      "type": "string",
      "required": false
    },
    "socials": {
      "type": "component",
      "repeatable": true,
      "component": "person.social-media"
    },
    "lang": {
      "type": "component",
      "repeatable": false,
      "component": "utilities.language"
    },
    "jobsExperience": {
      "type": "component",
      "repeatable": true,
      "component": "person.job-experience"
    },
    "jobs": {
      "type": "string",
      "required": true
    },
    "locations": {
      "type": "string",
      "required": true
    }
  }
}
